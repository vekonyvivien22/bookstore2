<% if (error) { %>
<div id="message" class="container">
  <br />
  <div class="alert alert-danger alert-dismissible text-center" role="alert">
    <button type="button" class="close" data-dismiss="alert">
      <span aria-hidden="true">&times;</span>
    </button>
    <strong> Error: </strong>
    <%= error %>
  </div>
</div>
<% } %>
